{
    "name": "cemerson/auth",
    "description": "A framework & data storage independent authentication library for PHP 7+",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Emerson",
            "email": "chris@cemerson.co.uk"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.222",
        "codercat/jwk-to-pem": "^1.1",
        "fig/http-message-util": "^1.1",
        "lcobucci/jwt": "^4.1",
        "paragonie/password_lock": "^3.1",
        "psr/clock": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master@dev",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.23"
    },
    "autoload": {
        "psr-4": {
            "CEmerson\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CEmerson\\Auth\\": "test",
            "spec\\CEmerson\\Auth\\": "spec"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "platform":  {
            "php": "7.4.0"
        }
    }
}
