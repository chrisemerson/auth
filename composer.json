{
    "name": "cemerson/auth",
    "description": "A framework & data storage independent authentication library for PHP 7",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Emerson",
            "email": "chris@cemerson.co.uk"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "cemerson/clock": "^1.2",
        "paragonie/password_lock": "^3.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "roave/security-advisories": "dev-master@dev",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "CEmerson\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\CEmerson\\Auth\\": "spec"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
